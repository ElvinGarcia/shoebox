
<header class="navbar navbar-fixed-top navbar-inverse">
        <div class="container">
          <nav>
          <% unless logged_in? %>  
          <ul class="nav navbar-nav navbar-right">
            <li> <%= link_to  "Home", root_path  %>   </li>
            <li> <%= link_to  "Log-in", login_path %> </li>
            <li> <%= link_to "Sign Up!", new_users_path, class: "btn btn-md btn-primary" %></li>
            <li> <%= link_to "Log-in with Github", "/auth/github/"%></li>
            <li> <%= link_to "Log-in with Google", "/auth/google_oauth2/"%></li>
            <% else %>
            <li><%= link_to 'Home', root_path %></li>
            <li><%= link_to 'Log Out', logout_path, method: :delete  if logged_in?%>
          </ul>
          <% end %>
          </nav>
        </div>
      </header>