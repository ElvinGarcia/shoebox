
<header class="navbar navbar-fixed-top navbar-inverse">
        <div class="container">
          <nav>  
          <ul class="nav navbar-nav navbar-right">
            <li> <%= link_to  "Home", root_path %> </li>
            <% unless logged_in? %>
            <li> <%= link_to "Sign Up!", new_user_path, class: "btn btn-md btn-primary" %></li>
            <li> <%= link_to  "Log-in", login_path %> </li>
            <li> <%= link_to "Log-in with Github", "/auth/github"%></li>
            <% else %>
            <li> <%= link_to  'Profile Page', user_path(current_user) %></li>
            <li><%= link_to 'Log Out', logout_path, method: :delete  if logged_in?%></li>
          </ul>
          <% end %>
          </nav>
        </div>
      </header>